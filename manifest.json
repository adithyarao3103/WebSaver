{
    "manifest_version": 3,
    "name": "WebSaver - Tab Manager with Notes",
    "version": "1.0",
    "description": "Save, tag, and organize your tabs with notes",
    "permissions": [
    "storage",
    "tabs"
    ],
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "128": "icons/icon128.png"
    }
    },
    "background": {
    "service_worker": "background.js"
    },
    "icons": {
    "128": "icons/icon128.png"
    }
}